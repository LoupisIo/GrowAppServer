{
    "plugins": ["plugins/markdown","jsdoc-route-plugin"],
    "recurseDepth": 10,
    "source": {
        "include":[".","./router","./controllers","./models"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts":{
        "destination":"./docs/",
        "template": "./custom-template"
    }
    
}